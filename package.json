{
    "name": "@obsercorma/nuxt-utilities",
    "version": "1.0.0",
    "description": "Shared utilities, composables, and helpers for Nuxt 4 projects.",
    "type": "module",
    "author": "Obsercorma",
    "exports": {
        ".": {
            "import": "./index.mjs",
            "types": "./index.d.ts"
        },
        "./*": {
            "import": "./*",
            "types": "./*.d.ts"
        }
    },
    "files": [
        "composables",
        "src",
        "index.ts",
        "dist"
    ],
    "main": "index.ts",
    "types": "./dist/index.d.ts",
    "scripts": {
        "build": "tsc",
        "prepare": "npm run build"
    },
    "peerDependencies": {
        "@nuxt/ui": "^3.3.0",
        "nuxt": "^4.0.1",
        "vue": "^3.5.x"
    },
    "devDependencies": {
        "@types/node": "^24.10.0",
        "typescript": "^5.9.3"
    },
    "repository": {
        "type": "git",
        "url": "git@github.com:Obsercorma/nuxt-utilities.git"
    }
}