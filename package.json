{
    "name": "@obsercorma/nuxt-utilities",
    "version": "1.0.0",
    "description": "Shared utilities, composables, and helpers for Nuxt 4 projects.",
    "type": "module",
    "author": "Obsercorma",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "types": "./dist/index.d.ts"
        },
        "./*": {
            "import": "./dist/*",
            "types": "./dist/*.d.ts"
        }
    },
    "files": [
        "dist"
    ],
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "scripts": {
        "build": "tsc",
        "postinstall": "node scripts/postInstall.js"
    },
    "peerDependencies": {
        "@nuxt/ui": "^3.3.0",
        "nuxt": "^4.0.1",
        "vue": "^3.5.x"
    },
    "devDependencies": {
        "@types/node": "^24.10.0",
        "typescript": "^5.9.3"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Obsercorma/nuxt-utilities.git"
    }
}